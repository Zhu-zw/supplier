/* pages/newlogin/newlogin.wxss */
page{
  height: 100%;
}
.content{
  height: 100%;
  width: 100%;
}
.regpic{
  width: 100%;
  height: 48%;
  float: left;
}
.con{
  width: 100%;
  height: 42%;
  float: left;
}
.foot{
  width: 100%;
  height: 10%;
  float: left;
}
.regpic image{
  width: 100%;
  height: 100%;
}
.enstyle{
  width: 84%;
  height: 100rpx;
  margin-left: 8%;
  border-bottom: #f2eeee solid 2rpx;
}
.sty-title{
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  float: left;
}
.sty-input{
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
  float: left;
}
.enrbtn{
  width: 84%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #066bd2;
  color: white;
  margin-top: 50rpx;
  margin-left: 8%;
  text-align: center;
  border-radius: 10rpx;
}
.foot-zc{
  width: 48%;
  height: 80rpx;
  margin-top: 10rpx;
  line-height: 80rpx;
  float: left;
  border-right: #787878 solid 3rpx;
  text-align: center;
}
.foot-wx{
  width: 80rpx;
  height: 80rpx;
  margin-top: 10rpx;
  line-height: 80rpx;
  float: left;
  text-align: center;
}
.foot-wx button{
  font-size:16px;
  color:#333;
  background-color:#fff;
}
.foot-wx image{
  width: 100%;
  height: 100%;
}